mixin players
    p players
    p #{res}
    ul
        each obj in res.players
            li #{obj.username}
mixin games
    p games
    ul
        each obj in res.games
            li #{obj.name}
mixin feeds
    p feeds
    ul
        each obj in res.feeds
            li #{obj.subject}
mixin comments(feedid)
    p comment
    ul
        each obj in res.comments
            li  #{obj.text}
mixin menu
    span(class="ui-state-default ui-corner-all")
        span(class="ui-icon ui-icon-grip-solid-horizontal")
    ul#menu
        li 
            div home
        li
            div games
            ul
                li 
                    div game1
                li 
                    div game2 
        li 
            div chats
            ul
                li 
                    div chat1
                li 
                    div  chat2
        li 
            div About